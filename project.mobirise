          "_name": "witsec-modal-window-block",
          "modalWindow": "<div class=\"modal fade\" id=\"modal1\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"modal1Label\" aria-hidden=\"true\">  <div class=\"modal-dialog  \" style=\"height:auto\" role=\"document\">    <div class=\"modal-content\"><div class=\"modal-header\">  <h5 class=\"no-anim modal-title\" id=\"modal1Label\">Modal Window</h5>  <a href=\"#\" class=\"no-anim close\" data-dismiss=\"modal\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></a></div><div class=\"modal-body\" id=\"modal1_body\">This is an example of a modal window. Download this Mobirise extension and add your own modal window now!</div><div class=\"modal-footer\"><div class=\"mbr-section-btn\"><a href=\"#\" class=\"no-anim btn btn-secondary display-4\" data-dismiss=\"modal\">Close</a></div></div>    </div>  </div></div><script>document.addEventListener(\"DOMContentLoaded\", function() {  $(\"#modal1\").on(\"hidden.bs.modal\", function () {    var html = $( \"#modal1_body\" ).html();    $( \"#modal1_body\" ).empty();    $( \"#modal1_body\" ).append(html);  })});</script>",
